    <string name="slide_one_top_text">Login with Google or Spotify</string>
    <string name="slide_one_down_text">This is the login page, where you can login with either your google or spotify account.</string>
    <string name="slide_two_top_text">Home page</string>
    <string name="slide_two_down_text">This is the home page, where you can scroll vertically and horizontally between your lyrics.</string>
